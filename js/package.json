{
  "name": "pocketscrape",
  "version": "5.0.1",
  "description": "Fetch Pocketcast feed",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "unit": "NODE_ENV=test mocha --recursive test/",
    "lint": "standard",
    "secscan": "nsp check",
    "coverage": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- --recursive test/",
    "test": "npm run lint && npm run secscan && npm run unit",
    "snapshot": "s3-cli --config s3cfg.ini sync data/snapshots/ s3://scrobblecast/snapshots/",
    "restore": "s3-cli --config s3cfg.ini sync s3://scrobblecast/snapshots/ data/snapshots/"
  },
  "author": {
    "name": "Daniel Lauzon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github.com:daneroo/scrobbleCast.git"
  },
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.3.10",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "crc": "^3.4.4",
    "cron": "^1.2.1",
    "express": "^4.15.3",
    "glob": "^7.1.2",
    "limiter": "^1.1.2",
    "lodash": "^2.4.2",
    "loggly": "^1.1.1",
    "mkdirp": "^0.5.0",
    "morgan": "^1.8.2",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "request-promise": "^4.2.1",
    "s3-cli": "^0.13.0",
    "sequelize": "^4.2.1",
    "sqlite3": "^3.1.8",
    "type-is": "^1.6.15",
    "winston": "^2.3.1",
    "winston-loggly": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "nsp": "^2.6.3",
    "sinon": "^2.3.6",
    "standard": "^10.0.2"
  },
  "standard": {
    "ignore": [],
    "env": [
      "mocha"
    ]
  }
}
